        "sourceMap": true,
        "typeRoots": ["node_modules/@types/"],
        "strict": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "strictPropertyInitialization": false,
        "alwaysStrict": true
        },
        "exclude": ["node_modules", "dist", "__tests__"]
}